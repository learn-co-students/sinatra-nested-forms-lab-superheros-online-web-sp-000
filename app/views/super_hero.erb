  <input id="member2_name" type="text" name="team[name][][hero_name]" /><br>
  <label for="member2_power">Type</label><br>
  <input id="member2_power" type="text" name="team[name][][power]" /><br>
  <label for="member2_bio">Booty</label><br>
  <input id="member2_bio" type="text" name="team[name][][bio]" /><br><br>
  
  <h2>Hero 3</h2>
  <label for="member3_name">Name</label><br>
  <input id="member3_name" type="text" name="team[name][][hero_name]" /><br>
  <label for="member3_power">Type</label><br>
  <input id="member3_power" type="text" name="team[name][][power]" /><br>
  <label for="member3_bio">Booty</label><br>
  <input id="member3_bio" type="text" name="team[name][][bio]" /><br><br>
  
  <input type="submit" value="submit">
</form>
