<div class="Team">
  <h1><%= @team.name %></h1>
  <h2>Team Motto: <%= @team.motto %></h2>
</div><br>

<% @members.each do |member| %>
  <div class="superhero">
    <h2><p>Hero Name: <%= member.name %></p></h2>
    <p>Hero Power: <%= member.power %></p>
    <p>Hero Biography: <%= member.bio %></p>
  </div><br>
<% end %>

