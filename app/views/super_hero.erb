        <h1>Create a Team and Heroes!</h1>
        <form action="/teams" method="POST">
            <label for="team_name">Team Name:</label><input type="text" name="team[name]" id="team_name"/><br>
            <label for="team_motto">Team Motto:</label><input type="text" name="team[motto]" id="team_motto"/><br>

            <h2>Hero 1</h2>
            <label for="member1_name">Hero Name:</label><input type="text" name="team[heros][][name]" id="member1_name"/><br>
            <label for="member1_power">Hero Power:</label><input type="text" name="team[heros][][power]" id="member1_power"/><br>
            <label for="member1_bio">Hero Biography:</label><input type="text" name="team[heros][][bio]" id="member1_bio"/><br>

            <h2>Hero 2</h2>
            <label for="member2_name">Hero Name:</label><input type="text" name="team[heros][][name]" id="member2_name"/><br>
