  <label for="team_motto">Weight</label><br>
  <input id="team_motto" type="text" name="team[motto]" /><br>
  

  <h2>Hero 1</h2>
  <label for="member1_name">Name</label><br>
  <input id="member1_name" type="text" name="team[name][][hero_name]" /><br>
