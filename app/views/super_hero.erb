<h1>Create a Team and Heroes!</h1>
<form action = '/team' method = 'POST'>
    <p>Team Name: <input type = "text" name = "team[name]">
    <p>Team Motto: <input type = "text" name = "team[motto]">
    <h3>Hero 1</h3>
    <p>Hero Name: <input type = "text" name = "team[heroes][][name]" id = "member1_name">
    <p>Hero Power: <input type = "text" name = "team[heroes][][power]" id = "member1_power">
    <p>Hero Biography: <input type = "text" name = "team[heroes][][bio]" id = "member1_bio">
    <h3>Hero 2</h3>
    <p>Hero Name: <input type = "text" name = "team[heroes][][name]" id = "member2_name">
    <p>Hero Power: <input type = "text" name = "team[heroes][][power]" id = "member2_power">
    <p>Hero Biography: <input type = "text" name = "team[heroes][][bio]" id = "member2_bio">
    <h3>Hero 2</h3>
    <p>Hero Name: <input type = "text" name = "team[heroes][][name]" id = "member3_name">
    <p>Hero Power: <input type = "text" name = "team[heroes][][power]" id = "member3_power">
    <p>Hero Biography: <input type = "text" name = "team[heroes][][bio]" id = "member3_bio">
    <input type = "submit" name = "submit" id = "submit">
</form>