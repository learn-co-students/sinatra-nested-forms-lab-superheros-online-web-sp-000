
<h1><%= @team.name %></h1>

<h2>Team Motto: <%= @team.motto %></h2>

<div class="heroes">
<%= @memberparams %>
 <% @members.each.with_index do |member| %>
     <div class="members">
         <h2>Hero Name: <%= member.name %></h2>
         <p>Hero Power: <%= member.power %></p>
         <p>Hero Biography: <%= member.bio %></p>
     </div>
    <% end %>


