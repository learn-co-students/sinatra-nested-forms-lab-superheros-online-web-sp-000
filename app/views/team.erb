
<h1> Heros</h1>
<% @heros.each do |hero| %>
   <p> <%= "Hero Name: #{hero.name}"%> </p>
   <p> <%= "Hero Power: #{hero.power}"%> </p>
   <p> <%= "Hero Biography: #{hero.biography}" %> </p>
<%end %>

<h1> Team </h1>

<% @teams.each do |team| %>
<p> <%= team.name%> </p>
<p> <%= "Team Motto: #{team.motto}" %> </p> 
 <%end %>




