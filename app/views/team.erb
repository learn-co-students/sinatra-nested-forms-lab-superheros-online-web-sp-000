<h1><%= @team_name %></h1><br>
<h2>Team Motto: <%= @team_motto %></h2><br>
<% @team_members.each.with_index do |hero, index| %>
    <h2>Hero Name: <%= @hero_name[index] %></h2><br>
    <p>
      Hero Power: <%= @hero_power[index] %><br>
      Hero Biography: <%= @hero_biography[index] %>
    </p>
  <% end %>



