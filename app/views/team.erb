<body>
<h1><%= @team.name %></h1>
<h2>Team Motto: <%= @team.motto %></h2>
<br>
<% i = 1 %>
<% @heroes.each do |hero| %>
<div class = "hero">
  <h2>Hero <%= i %>
  <p> <h2> Hero Name: <%= hero.name %> </h2> </p>
  <p> <h2> Hero Power: <%= hero.power %> </h2> </p>
  <p> <h2> Hero Biography: <%= hero.biography %> </h2> </p>
  <% i += 1 %>
<% end %>
</body>
