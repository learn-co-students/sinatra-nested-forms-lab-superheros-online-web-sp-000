<div class="team">
    <h1><%= @team[:name] %></h1>
    <h2>Team Motto: <%= @team[:motto] %></h2>
    </div><br>

<% @team[:members].each do |member| %>
<div class="hero">
    <h2>Hero Name: <%= member[:name] %></h2>
    <h3>Hero Power: <%= member[:power] %></h3>
    <h3>Hero Biography: <%= member[:bio] %></h3>
    </div>

<% end %>


